    apiKey: "<%= process.env.FIREBASE_API_KEY %>",
    authDomain: "message-stream-75839.firebaseapp.com",
    databaseURL:
      "https://message-stream-75839-default-rtdb.europe-west1.firebasedatabase.app",
    projectId: "message-stream-75839",
    storageBucket: "message-stream-75839.appspot.com",
    messagingSenderId: "40668410814",
    appId: "1:40668410814:web:bc275a0568aaf4afe13fef",
    measurementId: "G-6257DQMZDR",
  };

  // Initialize Firebase
  const app = initializeApp(firebaseConfig);
  const analytics = getAnalytics(app);
</script>

<!-- then our app's own js file -->
<script src="js/main.js"></script>
